@using WorldWideBasketball.Models;
@using System.Collections.Generic;
@model WorldWideBasketball.Models.LogingInfo;
@{
    ViewBag.Title = "Ligas";
}

Ola
<div class="text-center">
    <h1 class="display-4">Todas as Ligas:</h1>
    @foreach (KeyValuePair<string, List<Liga>> entry in (Dictionary<string, List<Liga>>)Model.getObject())
    {
        <div class="dropdown">
            <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                @entry.Key
            </button>

            <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                @foreach (Liga l in entry.Value)
                {
                    <li><a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Liga" asp-route-id="@l.Id">@l.Nome</a></li>

                }
            </ul>
        </div>
    }
</div>
